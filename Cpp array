#include <iostream> 

Using namespace std;

int main() { 

int A[100];

int n, sum = 0;

cout << "Enter size of array: "; 

cin >> n;

for (int i = 0; i < n; i++) {

cout << "Enter array element: "; 

cin >> A[i];

}

for (int i = 0; i < n; i++) { 

sum += A[i];

}

cout << "Summation of Array is = " << sum << endl; 

return 0;

}
